{
  "manifest_version": 2,
  "name": "Рейтинг фильмов IMDb",
  "description": "Позволяет отображать рейтинг фильмов и сериалов на сайте FS.to",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://fs.to/video*"
      ],
      "js": [
        "lib/jquery.min.js",
        "fs_inject.js",
        "src/IMDb.rating.js",
        "src/kp.rating.js"
      ],
      "css": [
        "fs_styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/*"
  ]
}